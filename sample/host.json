{
  "version": "2.0",
  "logging": {
    "logLevel": {
      "Function": "Information",
      "default": "None"
    }
  },
  "extensions": {
    "eventHub": {
      "maxBatchSize": 10,
      "prefetchCount": 256,
      "batchCheckpointFrequency": 1
    },
    "serviceBus": {
      "maxConcurrentCalls": 10,
      "prefetchCount": 100,
      "autoRenewTimeout": "00:05:00"
    }
  }
}
